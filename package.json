{
  "name": "assets-manifest-parser",
  "version": "0.1.0",
  "description": "A Node.js CLI tool for parsing binary manifest files generated by yooasset",
  "type": "module",
  "scripts": {
    "start": "tsx src/cli.ts",
    "format:prettier": "prettier --write src",
    "lint:eslint": "pnpm lint:tsc && eslint --ext .ts src",
    "lint:tsc": "tsc --noEmit",
    "build": "tsc --project tsconfig.json --outDir dist",
    "prepack": "pnpm run build",
    "prepare": "husky"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "yooasset",
    "cli"
  ],
  "author": "median-dxz",
  "repository": {
    "url": "https://github.com/median-dxz/assets-manifest-praser"
  },
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.12.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript-eslint": "^8.32.1"
  },
  "lint-staged": {
    "src/*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}