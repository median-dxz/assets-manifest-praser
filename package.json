{
  "name": "assets-manifest-parser",
  "version": "0.1.0",
  "description": "A Node.js CLI tool for parsing manifest files generated by yooasset",
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json"
  },
  "keywords": [
    "yooasset"
  ],
  "author": "median-dxz",
  "repository": {
    "url": "https://github.com/median-dxz/assets-manifest-praser"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "dependencies": {
    "binary-parser": "^2.2.1",
    "citty": "^0.1.6",
    "consola": "^3.4.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.12.0",
    "tsx": "^4.19.4"
  },
  "nodemonConfig": {
    "watch": [
      "./tsconfig.json",
      "./src/**/*",
      ".env"
    ],
    "ext": "ts,js",
    "execMap": {
      "ts": "tsx"
    }
  }
}